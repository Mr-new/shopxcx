/**index.wxss**/
.swiper{
  width: 100%;
  /* height: 360rpx; */
}
.slide-image{
  width: 100%;
  display: block;
}
.menu{
  width: 96%;
  padding: 20rpx 2%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  /* border-bottom: 15rpx solid #F2F0F3; */
}
.menu>navigator{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 20%;
  margin: 15rpx 0;
}
.menu>navigator>image{
  width: 60%;
}
.menu>navigator>text:first-of-type{
  font-size: 10pt;

}
.menu>navigator>text:nth-of-type(2){
  font-size: 4pt;
}
.top{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.top>navigator>image:first-of-type{
  width: 100%;
  margin-top: 20rpx;
}
.top>view{
  width: 100%;
  height: auto;
  overflow: hidden;
  background-color: #FAE2F2;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.top>view>text{
  font-size: 12pt;
  color: #A661AD;
  line-height: 100rpx;
}
.top>view>view{
  width: 95%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.top>view>view>navigator{
  width: 31%;
}
.top>view>view>navigator>image{
  width: 100%;
}
.header{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 30rpx 0;
}
.header>text{
  font-size: 13pt;
  color: #333333;
}
.header>image{
  width: 20%;
}
.list{
  width: 94%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 30rpx 3% 0;
}
.goodsSwiper{
  width: 100%;
}
.goodsPage{
  display: flex;
  width: 100%;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
}
.goodsPage>navigator{
  width: 31.33%;
  margin: 10rpx 1%;

}
.goodsPage>navigator>image{
  width: 100%;
  display: block;
}
