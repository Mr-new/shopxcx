.itemBottom>view:first-of-type>image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.itemBottom>view:first-of-type>text{
  width: 60%;
  font-size: 9pt;
  color: #666666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.itemBottom>view:last-of-type{
  width: 35%;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.itemBottom>view:last-of-type>text:last-of-type{
  font-size: 10pt;
  margin-left: 4rpx;
  color: #666666;
}

