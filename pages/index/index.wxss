/* 拼团、秒杀、砍价模块 */
.activityImg{
  width: 94%;
  margin: 20rpx 3%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.activityImg>view:first-of-type{
  width: 44%;
  
}
.activityImg>view:first-of-type>navigator{
  width: 100%;
}
.activityImg>view:first-of-type>navigator>image{
  display: block;
  width: 100%;
}
.activityImg>view:last-of-type{
  width: 54.5%;
  display: flex;
  flex-direction: column;
}
.activityImg>view:last-of-type>navigator{
  width: 100%;
}
.activityImg>view:last-of-type>navigator:first-of-type{
  margin-bottom: 15rpx;
}
.activityImg>view:last-of-type>navigator>image{
  width: 100%;
  display: block;
}
/* 周年庆活动 */
.bannerImg{
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}
.bannerImg>image:first-of-type{
  width: 100%;
  display: block;
}
.bannerImg>image:last-of-type{
  display: block;
  width: 300rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -150rpx;
  margin-top: 20rpx;
  animation: sf 1.3s linear infinite;
}
@keyframes sf{
  0%{
    transform: scale(1);
  }
  50%{
    transform: scale(1.2);
  }
  100%{
    transform: scale(1);
  }
}
.hdback{
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}
.hdback>image{
  width: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.huichang{
  width: 94%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-end;
}
.huichang>navigator:nth-of-type(1),.huichang>navigator:nth-of-type(3){
  flex: 1;
  margin: 0 5rpx;
}
.huichang>navigator:nth-of-type(2){
  flex: 1.4;
  /* margin: 0 5rpx; */
  margin-left: 10rpx;
}
.huichang>navigator>image{
  display: block;
  width: 100%;
}
/* 固定在屏幕顶部菜单 */
.headerFixed{
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
  /* background-color: #39013E; */
}
.headerFixed>view:first-of-type{
  width: 94%;
  margin: 20rpx auto 0;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.headerFixed>view:first-of-type>image{
  width: 20%;
  display: block;
  margin-right: 30rpx;
}
.headerFixed>view:first-of-type>text{
  font-size: 12pt;
  color: #ffffff;
}
.navList{
  width: 100%;
  height: 70rpx;
  padding-bottom: 10rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.navList>scroll-view{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  white-space: nowrap;
}
.navItem{
  display: inline-flex;
  justify-content: center;
  align-items: center;
  /* padding: 0 30rpx; */
  margin: 0 40rpx;
  height: 68rpx;
  font-size: 11pt;
  color: #FFFFFF;
}
.selected{
  color: #FFFFFF;
  font-weight: bold;
  font-size: 11pt;
  border-bottom: 6rpx solid #FFFF69;
  box-sizing: border-box;
}
.imgWrap{
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}
.imgWrap>image{
  display: block;
  width: 100%;
}
.imgWrap>view{
  width: 90%;
  height: auto;
  overflow: hidden;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 99;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-end;
}
.imgWrap>view>text{
  padding: 5rpx 15rpx;
  background-color: rgba(0, 0, 0, 0.5);
  color: #ffffff;
  font-size: 8pt;
  border-radius: 20rpx;
}
.imgWrap>view>image{
  display: block;
  width: 130rpx;
  border-top: 4rpx solid #ffffff;
  border-left: 4rpx solid #ffffff;
  border-top-left-radius: 10rpx;
}
/* 周年庆弹框 */
.tankuang{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 99999;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tankuang>image:first-of-type{
  width: 70%;
  display: block;
}
.tankuang>image:last-of-type{
  width: 50rpx;
  display: block;
  margin-top: 40rpx;
}
/* 右边侧栏 */
.rightMenu{
  width: 120rpx;
  height: 540rpx;
  /* overflow: hidden; */
  display: flex;
  flex-direction: column;
  position: fixed;
  bottom: 20rpx;
  right: 20rpx;
  z-index: 9999;
}
.rightMenu>button{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  line-height: 1;
  font-size: 11pt;
  position: absolute;
  bottom: 0;
  right: 0;
}
.rightMenu>button:first-of-type{
  animation: menu3 0.5s linear forwards;
  
}
.rightMenu>button:nth-of-type(2){
  animation: menu2 0.5s linear forwards;
}
.rightMenu>button:nth-of-type(3){
  
  animation: menu1 0.5s linear forwards;
}
@keyframes menu1{
  0%{
    bottom: 0;
  }
  100%{
    bottom: 140rpx;
  }
}
@keyframes menu2{
  0%{
    bottom: 0;
  }
  100%{
    bottom: 280rpx;
  }
}
@keyframes menu3{
  0%{
    bottom: 0;
  }
  100%{
    bottom: 420rpx;
  }
}
.rightMenu>button>image{
  width: 60rpx;
  display: block;
  margin-bottom: 5rpx;
}
.rightMenu>button>text{
  font-size: 10pt;
  color: #363636;
}
.rightMenu>button:last-of-type{
  width: 110rpx;
  height: 110rpx;
  right: 5rpx;
  background-color: rgba(0, 0, 0, 0.5) !important;
}
.rightMenu>button:last-of-type>text{
  font-size: 17pt;
  color: #ffffff;
}
