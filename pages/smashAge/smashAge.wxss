/**index.wxss**/
page{
  background-color: #AD1C14;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
}
.top{
  width: 100%;
  height: auto;
  /* overflow: hidden; */
  position: relative;
}
.top>image{
  width: 100%;
}
.wwwrap{
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  /* background-color: red; */
}
.marqueen{
  animation: marqueen 8s linear infinite;
}
@keyframes marqueen{
  0%{
    transform: translateY(0)
  }
  100%{
    transform: translateY(-100%)
  }
}
.wrap{
  width: 100%;
  height: auto;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
}
.content{
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  /* background-color: red; */
}
.rule{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #E7E7E7;
  width: 90rpx;
  height: 40rpx;
  border-radius: 40rpx;
  box-shadow: 0 4rpx 0 #631411;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}
.rule>text{
  font-size: 8pt;
  color: #B0241D;
}
.title{
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 80rpx;
  position: relative;
  /* background-color: gray; */
  margin-bottom: 50rpx;
}
.title>image{
  width: 100%;
}
.ageList{
  width: 95%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
  transform: translateY(-60rpx);
}
.ageList>button{
  width: 32%;
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: none;
  outline: none;
}
.ageList>button::after{
  border: none;
  outline: none;
}
.ageList>button>image{
  width: 100%;
}
.ageList>image{
  width: 29%;
  margin: 10rpx 0;
}
.tips{
  display: flex;
  flex-direction: column;
  width: 100%;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 50rpx;
  left: 0;
  /* transform: translateY(-150rpx); */
  /* padding: 20rpx 0 30rpx; */
}
.tips>text:first-of-type{
  font-size: 11pt;
  color: #A4A4EE;
}
.tips>text:nth-of-type(1){
  padding: 10rpx 30rpx;
  border: 2rpx solid #ffffff;
  font-size: 9pt;
  color: #ffffff;
  border-radius: 20rpx;
  margin-top: 10rpx;
}

@media screen and (max-width: 375px) {
  /* .tips{
    padding: 70rpx 0 20rpx;
  } */
  .text>view>text{
    font-size: 9pt !important;
  }
}
@media screen and (max-width: 320px) {
  /* .tips{
    padding: 40rpx 0 20rpx;
  } */
  .text>view>text{
    font-size: 8pt !important;
  }
  
}
