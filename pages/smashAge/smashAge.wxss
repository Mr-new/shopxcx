.activity_role{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,0.6);
  z-index: 99;
  display: flex;
  justify-content: center;
  /* align-items: center; */
}
.activity_role>view{
  width: 80%;
  height: auto;
  overflow: hidden;
  transform: translateY(-100%);
  animation: topBottom 2s linear forwards;
}
@keyframes topBottom{
  0%{
    transform: translateY(-100%);
  }
  50%{
    transform: translateY(0);
  }
  60%{
    transform: translateY(-20%);
  }
  70%{
    transform: translateY(-5%);
  }
  80%{
    transform: translateY(-15%);
  }
  90%{
    transform: translateY(-5%);
  }
  100%{
    transform: translateY(0);
  }
}
