/**index.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
}
.header{
  width: 100%;
  
}
.search{
  width: 96%;
  padding: 20rpx 2%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.search>view:first-of-type{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 15%;
}
.search>view:first-of-type>text:first-of-type{
  font-size: 12pt;
  color: #F19B48;
  margin-right: 5%;
}
.search>view:first-of-type>text:nth-of-type(2){
  font-size: 11pt;
  color: #999999;
}
.search>view:nth-of-type(2){
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 70%;
  height: 60rpx;
  background-color: #F5F5F5;
  border:2rpx solid #EEEEEE;
  border-radius: 8rpx;
}
.search>view:nth-of-type(2)>text{
  font-size: 14pt;
  color: #BEBEBE;
  margin: 0 2%;
}
.search>view:nth-of-type(2)>input{
  width: 90%;
  font-size: 10pt;
  color: #959595;
}
.search>view:nth-of-type(3){
  width: 12%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search>view:nth-of-type(3)>text{
  font-size: 11pt;
  color: #999999;
}
.navList{
  width: 92%;
  padding: 0 3%;
  height: 70rpx;
  border-bottom: 2rpx solid #DCDCDC;
}
.navList>scroll-view{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  white-space: nowrap;
}
.navItem{
  display: inline-flex;
  justify-content: center;
  align-items: center;
  padding: 0 30rpx;
  height: 68rpx;
  font-size: 10pt;
  color: #333333;
}
.selected{
  color: #E45452;
  border-bottom: 4rpx solid #E45452;
  box-sizing: border-box;
}
.caseList{
  width: 100%;
  height: 84%;
}
.listItem{
  display: flex;
  width: 100%;
  flex-direction: column;
  border-bottom: 15rpx solid #F5F5F5;
}
.top{
  width: 92%;
  padding: 30rpx 3%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.top>image{
  width: 80rpx; 
  height: 80rpx;
  border-radius: 50%;
}
.top>view{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  margin-left: 20rpx;
  width: 80%;
}
.top>view>text:first-of-type{
  width: 100%;
  font-size: 11pt;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.top>view>text:nth-of-type(2){
  font-size: 8pt;
  color: #ffffff;
  padding: 5rpx 10rpx;
  background-color: #F4BC50;
  border-radius: 8rpx;
}
.center{
  width: 92%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 0 3%;
}
.center>view:first-of-type{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.center>view:first-of-type>image{
  width: 32%;
}
.center>view:nth-of-type(2){
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}
.center>view:nth-of-type(2)>text{
  font-size: 9pt;
  color: #999999;
}
.center>view:nth-of-type(2)>view{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  transform: translateX(-18rpx);
}
.center>view:nth-of-type(2)>view>text:first-of-type{
  color: #E45452;
  font-size: 10pt;
  margin-right: 8rpx;
}
.center>view:nth-of-type(2)>view>text:nth-of-type(2){
  color: #326B73;
  font-size: 10pt;
  line-height: 60rpx;
}
.bottom{
  display: flex;
  width: 100%;
  padding: 15rpx 0;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.bottom>view{
  width: 33.33%;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bottom>view>text:first-of-type{
  font-size: 10pt;
  color: #434343;
  margin-right: 5rpx;
}
.bottom>view:first-of-type>text:first-of-type{
  font-size: 12pt;
}
.bottom>view>text:nth-of-type(2){
  color: #333333;
  font-size: 10pt;
}
.line{
  width: 100%;
  height: 2rpx;
  background-color: #EAEAEA;
}