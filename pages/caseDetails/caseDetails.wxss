/* 变美过程 */
.process{
  width: 96%;
  padding: 20rpx 2%;
  height: auto;
  overflow: hidden;
  background-color: #ffffff;

}
.process_title{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.process_title>text{
  font-size: 12pt;
  color: #333333;
  font-weight: bold;
}
.process_title>view>text{
  font-size: 10pt;
  color: #575757;
}
.processList{
  width: 100%;
  /* height: auto;
  overflow: hidden; */
}
.processItem{
  width: 97%;
  padding-left: 3%;
  border-left: 2rpx solid #C7C7C7;
  height: auto;
  position: relative;
  margin: 30rpx 0;
}
.processItem::before{
  position: absolute;
  top: -9rpx;
  left: -9rpx;
  content: '';
