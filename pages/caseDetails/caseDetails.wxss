  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  border: 4rpx solid #B02525;
  background-color: #ffffff;
}
.processItem>text:first-of-type{
  font-size: 10pt;
  color: #333333;
  display: block;
  transform: translateY(-17rpx);
}
.process_video{
  width: 100%;
}
.process_video>video{
  width: 100%;
  border-radius: 10rpx;
}
.content{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.process_imglist{
  width: 100%;
}
.process_imglist>view>image{
  border-radius: 10rpx;
  display: block;
}
.imagelist1{
  width: 100%;
}
.imagelist1>image{
  width: 100%;
  
}
.imagelist2{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.imagelist2>image{
  width: 49%;
  height: 335rpx;
}
.imagelist3{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.imagelist3>image{
  width: 32.33%;
  height: 230rpx;
}
.imagelist4{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}
.imagelist4>image{
  width: 48%;
  margin: 7rpx 1%;
  height: 335rpx;
}
.imagelist5{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}
.imagelist5>image:nth-of-type(1),.imagelist5>image:nth-of-type(2){
  width: 48%;
  margin: 7rpx 1%;
  height: 335rpx;
}
.imagelist5>image:nth-of-type(3),.imagelist5>image:nth-of-type(4),.imagelist5>image:nth-of-type(5){
