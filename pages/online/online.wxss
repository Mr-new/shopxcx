/* online.wxss */
page{width:100%;height:100%;}
.container_online{
  width:100%;
  height:100%;
  
  overflow:hidden;
  background: url(https://wxxcx.yixingtb.com/images/online_bg.jpg?id=9);
  background-size:100% 100%;
}

.Myfrom{width:90%;margin:40rpx auto 0; background:#fff;display: block;padding:30rpx 20rpx;border-radius:25rpx;font-size:28rpx;}
.form-text{width: 100%;height:60rpx;line-height:60rpx;color: #333;margin:20rpx auto;position:relative;}
.form-text label{float: left;height:100%; width: 28%;}
.form-text .input{width: 70%;height:60rpx;line-height:58rpx;float: left;border: #c6c6c6 1rpx solid;padding-left:15rpx;box-sizing:border-box;border-radius:10rpx;}
.form-text .input .dateoption{line-height:60rpx;}
.form-text .textarea{width:100%;height:150rpx;padding:20rpx 15rpx;box-sizing:border-box; margin-bottom: 10rpx;}
.form-text .getPhoneNumber{width:160rpx;height:50rpx;line-height:50rpx; background-color:#09bb07;color:#ffffff;font-size:24rpx; position:absolute;right:17rpx;top:50%;margin-top:-25rpx;z-index:33;border-radius:10rpx;}

.from_submit{width:500rpx;height: 80rpx;line-height:80rpx;margin:0 auto;background-color: #b0875b;color: #fff;letter-spacing: 2px;border-radius:10rpx;}
.checkbox1{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.checkbox1>label{
  width: 168rpx;
  margin-bottom: 10rpx;
  font-size: 10pt;
}
.checkbox1>label>checkbox{
  transform: scale(0.7)
}